package org.teachingkidsprogramming.CodingChallenges;

public class print10integersSameLine
{
  public static void main(String[] args)
  {
    for (int i = 0; i < 10; i++)
    {
      System.out.print(i + " ");
      System.out.println();
    }
    
    for (int i = 0; i < 11; i++)
    {
      System.out.print(i + " ");
    }
  }
}
